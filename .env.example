# Example .env file for the Gifroz project
# Replace the placeholder values with your actual API keys

LOG_LEVEL=DEBUG

# giphy rate limit: 100 API request per hours
GIPHY_API_KEY=your_giphy_api_key_here

# tenor rate limit: 1 API request per second (RPS)
TENOR_API_KEY=your_tenor_api_key_here

CACHE_TIMEOUT=300