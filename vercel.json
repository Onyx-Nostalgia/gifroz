{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "buildCommand": "python3 -m pip install uv && python3 -m  uv lock ",
  "installCommand": "python3 -m  uv sync --all-groups",
  "devCommand": "python3 -m uv run -- flask run", 
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}